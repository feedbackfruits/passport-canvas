{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "extension": [
    ".ts"
  ],
  "exclude": [
    "dist/**",
    "test/**",
    "functions/**",
    "migrations/**",
    "node_modules/**",
    "debug/**",
    "coverage/**",
    "**/*.spec.ts",
    ".eastrc.js"
  ],
  "reporter": [
    "json",
    "text"
  ]
}
